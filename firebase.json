{
	"database": {
		"rules": "database.rules.json"
	},
	"hosting": {
		"public": "public",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"redirects": [{
			"source": "/",
			"destination": "/index.html",
			"type": 301
		},{
			"source": "/home.html",
			"destination": "/index.html",
			"type":301
		}]
	},
	"storage": {
		"rules": "storage.rules"
	}
}